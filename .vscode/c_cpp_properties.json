{
    "configurations": [
        {
            "name": "macos-clang",
            "includePath": [
                "${workspaceFolder}/libsrc",
                "${workspaceFolder}",
                "${workspaceFolder}/tests/data",
                "${env:GEOMODELGRIDS_BUILDDIR}",
                "${env:PYBIND11_INCDIR}",
                "${env:PYTHON_INCDIR}",
                "${env:MPI_INCDIR}",
                "${env:HDF5_INCDIR}",
                "${env:PROJ_INCDIR}",
                "${env:GDAL_INCDIR}",
                "${env:CATCH2_INCDIR}"
            ],
            "defines": [
                "WITH_GDAL"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "macos-clang-x64"
        },
        {
            "name": "linux-gcc",
            "includePath": [
                "${workspaceFolder}/libsrc",
                "${workspaceFolder}",
                "${workspaceFolder}/tests/data",
                "${env:GEOMODELGRIDS_BUILDDIR}",
                "${env:PYBIND11_INCDIR}",
                "${env:PYTHON_INCDIR}",
                "${env:MPI_INCDIR}",
                "${env:HDF5_INCDIR}",
                "${env:PROJ_INCDIR}",
                "${env:GDAL_INCDIR}",
                "${env:CATCH2_INCDIR}"
            ],
            "defines": [
                "WITH_GDAL"
            ],
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}